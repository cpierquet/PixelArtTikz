\NewDocumentCommand\TraiteListeSymbPixelArt{ m }{%
	\IfSubStr{#1}{§}%
		{%
			\setsepchar{§}%
			\readlist*\lstPAchoices{#1}%
			\xdef\lstPAchoicesrand{\fpeval{randint(1,\lstPAchoiceslen)}}%
			\lstPAchoices[\lstPAchoicesrand]%
		}%
		{%c'est normal
			#1%
		}%
}

